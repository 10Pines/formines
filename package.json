{
  "name": "formosa",
  "version": "0.0.0",
  "description": "Simple and composable validations for react",
  "url": "https://github.com/10Pines/formosa",
  "scripts": {
    "setup": "lerna bootstrap",
    "clean": "lerna run --no-private clean",
    "test": "lerna run --no-private test",
    "build": "lerna run --no-private build",
    "publish:raw": "lerna run --no-private npm publish",
    "publish": "run-s clean build publish:raw"
  },
  "private": true,
  "dependencies": {
    "lerna": "^3.4.3",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.3"
  }
}
