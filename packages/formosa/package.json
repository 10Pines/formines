    "jest-plugin-set": "^2.9.0",
    "jest-plugins": "^2.9.0",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.3.2"
  },
  "dependencies": {
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2"
