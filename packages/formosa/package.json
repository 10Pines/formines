  "description": "Simple and composable validations for react",
  "author": "Lucas David Traverso <lucas6246@gmail.com>",
  "license": "GPL-3.0",
  "repository": "https://github.com/10Pines/formosa/tree/master/packages/formosa",
  "scripts": {
